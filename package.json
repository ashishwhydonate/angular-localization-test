{
  "name": "localize",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run localize:serve-ssr",
    "serve:ssr": "node dist/localize/server/main.js",
    "build:ssr": "ng build && ng run localize:server",
    "prerender": "ng run localize:prerender",
    "process": "node ./tools/copy-worker-files.mjs && node ./tools/copy-client-files.mjs && node ./tools/bundle.mjs",
    "prestart": "npm run build:ssr && npm run process",
    "predeploy": "npm run process",
    "deploy": "wrangler pages deploy dist/cloudflare"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@gilsdav/ngx-translate-router": "^6.1.0",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@nguniversal/express-engine": "^16.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "async-hooks": "^1.3.1",
    "bootstrap": "^5.2.3",
    "express": "^4.15.2",
    "ngx-bootstrap": "^11.0.2",
    "rxjs": "^7.6.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular-eslint/builder": "16.0.1",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular-eslint/schematics": "16.0.1",
    "@angular-eslint/template-parser": "16.0.1",
    "@angular/cli": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.16.5",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  }
}
